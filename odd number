package SampleProject;
import java.util.Scanner;

public class SampleClass{
	
	public static void main(String[] args) {
	
      
      for(int i=1; i<=100; i++)
      {
    	  if(i%2!=0) {
    		  System.out.println(i);
    	  }
    	  
      }
      
    
}

}



package JavaCode;

public class JavaCode{
	public static void main(String[] args) {     // 8/4/2022
		
		
	for (int i = 1; i<=50; i++)
	{
		if(i%2!=0) {
			System.out.println(i);
		}
	}
	}
	
}
		
	
