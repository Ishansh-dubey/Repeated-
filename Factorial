package SampleProject;

import java.util.Scanner;

public class SampleClass{
	
	public static void main (String[] args) {
     
	      int a=1;
	       
	      Scanner sc = new Scanner(System.in);
	      
	      System.out.println("number :");
	      int b = sc.nextInt();
	 
	     
	      for(int i = b; i > 0; i--) {
	         
	          a = a*i;
	      }
	    
	      
	         System.out.println("Factorial: "+a);
	 }
}
							

	
	
