	    	for( int i=1; i<= row;  ) {
	    		for(  int s=1; s<=i; ) {
	   
	    			System.out.print(z+" ");
	    		     s++;
	    		}
	    			i++;	
                z=z+1;
	    		System.out.println();
	    	}
	    	

	     }
	}







package SampleProject;

public class SampleClass{
	

	    public static void main(String []args){    
	
	          int row = 5;  
	          int j=1;
	         int z=65;
	        
	    	for( int i=1; i<= row;  ) {
	    		for(  int s=1; s<=i; ) {
	    			String y = Character.toString(z);
	    			System.out.print(y+" ");
	    		     s++;
	    		}
	    			i++;	
                z=z+1;
	    		System.out.println();
	    	}
	    	

	     }
	}
	
	
	
	
	
	
	package SampleProject;

public class SampleClass{
	

	    public static void main(String []args){    
	
	          int row = 5;  
	          int j=1;
	         int z=65;
	        
	    	for( int i=1; i<= row;  ) {
	    		for(  int s=1; s<=i; ) {
	    			String y = Character.toString(z);
	    			System.out.print(y+" ");
	    		     s++;
	    		     z=z+1;
	    		}
	    			i++;	
               
	    		System.out.println();
	    	}
	    	

	     }
	}
	
	
	
	
	
	
	
	
	package SampleProject;

public class SampleClass{  
	public static void main(String[] args){ 

		int num =  1;
		int rows = 10;
		int cols = 11;
		
		for( int i = 0; i<=rows; i++) 
		{
			for(int j = 0; j<=cols; j++)
			{
				if (i+j <= cols -1 ) 
				{
					System.out.print("  ");
				}
				else {
					System.out.print(" " + num);
					num++;
				}
				
			}
			System.out.println();
		}
		
	}
}





