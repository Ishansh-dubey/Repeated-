package SampleProject;

public class SampleClass{
	public static void main (String[]args) {
		
		int a=1;
		int b=1;
		int c=1;
		int res=0;
		if (a==b || b==c || a==c) {
		  if(a==b) {
			  res = a+b;
			  System.out.println("Result ab = "+res);
		  }
		  if (b==c) {
			  res = b+c;
			  System.out.println("Result bc = "+res);
		  }
		  if (a==c) {
			  res = a+c;
			  System.out.println("Result ac = "+res);
		  }
		}
	  else {
		  System.out.println("Result  = "+res);
	  }
		
		
	}
}
